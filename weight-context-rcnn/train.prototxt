  bottom: "place_fc6"
  top: "place_fc6"
  dropout_param {
    dropout_ratio: 0.5
    scale_train: false
  }
}
layer {
  name: "place_fc7"
  type: "InnerProduct"
  bottom: "place_fc6"
  top: "place_fc7"
  param { lr_mult: 0 decay_mult: 0}
  param { lr_mult: 0 decay_mult: 0}
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "place_relu7"
  type: "ReLU"
  bottom: "place_fc7"
  top: "place_fc7"
}
layer {
  name: "place_drop7"
  type: "Dropout"
  bottom: "place_fc7"
  top: "place_fc7"
  dropout_param {
    dropout_ratio: 0.5
    scale_train: false
  }
}

layer {
  name: "replicabath"
  type: "Python"
  bottom: "roi_pool5"
  bottom: "place_fc7"
  top: "replicabath"
  python_param {
    module: 'rpn.duplicate_bath'
    layer: 'DuplicateBathLayer'
  }
}

# Dummy layers so that initial parameters are saved into the output net

layer {
  name: "dummy_roi_pool_conv5"
  type: "DummyData"
  top: "dummy_roi_pool_conv5"
  dummy_data_param {
    shape { dim: 1 dim: 4096 }
    data_filler { type: "constant" value: 0 }
  }
}

layer {
  name: "place_fc8a"
  type: "InnerProduct"
  bottom: "dummy_roi_pool_conv5"
  top: "place_fc8a"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  inner_product_param {
    num_output: 365
  }
}
layer {
  name: "cls_score"
  type: "InnerProduct"
  bottom: "dummy_roi_pool_conv5"
  top: "cls_score"
  param { lr_mult: 0 decay_mult: 0}
  param { lr_mult: 0 decay_mult: 0}
  inner_product_param {
    num_output: 21
  }
}
layer {
  name: "silence_fc8"
  type: "Silence"
  bottom: "place_fc8a"
}

layer {
  name: "silence_cls_score"
  type: "Silence"
  bottom: "cls_score"
}


# ===== fusion ====

layer {
  name: "weight_feats"
  type: "Python"
  bottom: "fc7"
  bottom: "replicabath"
  top: "wfc7"
  top: "wrepbath"
  python_param {
    module: 'fast_rcnn.weight_feats'
    layer: 'WeightLayer'
  }
}



layer {
   name: "concat"
   bottom: "wfc7"
   bottom: "wrepbath"
   top: "concat"
   type: "Concat"
   concat_param {
    axis: 1
  }
}

layer {
  name: "context_cls_score"
  type: "InnerProduct"
  bottom: "concat"
  top: "context_cls_score"
  param { lr_mult: 1 }
  param { lr_mult: 2 }
  inner_product_param {
    num_output: 21
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

layer {
  name: "bbox_pred"
  type: "InnerProduct"
  bottom: "fc7"
  top: "bbox_pred"
  param { lr_mult: 0 decay_mult: 0}
  param { lr_mult: 0 decay_mult: 0}
  inner_product_param {
    num_output: 84
  }
}
layer {
  name: "loss_cls"
  type: "SoftmaxWithLoss"
  bottom: "context_cls_score"
  bottom: "labels"
  propagate_down: 1
  propagate_down: 0
  top: "cls_loss"
  loss_weight: 1
}
layer {
  name: "loss_bbox"
  type: "SmoothL1Loss"
  bottom: "bbox_pred"
  bottom: "bbox_targets"
  bottom: "bbox_inside_weights"
  bottom: "bbox_outside_weights"
  top: "bbox_loss"
  loss_weight: 1
}

